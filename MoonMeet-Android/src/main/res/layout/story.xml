<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="#FFFFFF"
		android:gravity="center_horizontal"
		android:orientation="vertical"
		android:fitsSystemWindows="true">
		<ProgressBar
			android:id="@+id/progressbar1"
			android:layout_width="30dp"
			android:layout_height="wrap_content"
			android:indeterminate="false"
			style="?android:progressBarStyle" />
		<LinearLayout
			android:id="@+id/linear14"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:paddingLeft="2dp"
			android:paddingRight="2dp"
			android:background="#FFFFFF"
			android:gravity="center_vertical"
			android:orientation="horizontal"
			android:fitsSystemWindows="true">
			<ImageView
				android:id="@+id/imageview8"
				android:layout_width="50dp"
				android:layout_height="50dp"
				android:layout_marginTop="4dp"
				android:layout_marginBottom="4dp"
				android:padding="14dp"
				android:src="@drawable/ic_close_white"
				android:scaleType="centerInside"
				android:alpha="0.8" />
			<LinearLayout
				android:id="@+id/linear16"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/propics_lin"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<de.hdodenhof.circleimageview.CircleImageView
						android:id="@+id/circleimageview1"
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:src="@drawable/icplaceholder" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear15"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:orientation="vertical"
					android:layout_weight="1">
					<TextView
						android:id="@+id/textview8"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="10dp"
						android:paddingRight="8dp"
						android:text="MoonMeet User"
						android:textSize="16sp"
						android:textStyle="bold"
						android:textColor="#000000"
						android:alpha="0.8" />
					<TextView
						android:id="@+id/textview9"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="10dp"
						android:paddingRight="8dp"
						android:text="Time"
						android:textSize="12sp"
						android:textColor="#000000"
						android:alpha="0.3" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear20"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="vertical"
				android:layout_weight="1" />
			<LinearLayout
				android:id="@+id/linear19"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="1dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview13"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="4dp"
					android:paddingBottom="4dp"
					android:gravity="center_horizontal|center_vertical"
					android:text="0"
					android:textSize="14sp"
					android:textColor="#000000" />
				<ImageView
					android:id="@+id/imageview9"
					android:layout_width="34dp"
					android:layout_height="34dp"
					android:padding="8dp"
					android:src="@drawable/ic_remove_red_eye_white"
					android:scaleType="fitCenter" />
			</LinearLayout>
			<ImageView
				android:id="@+id/imageview11"
				android:layout_width="50dp"
				android:layout_height="50dp"
				android:padding="14dp"
				android:src="@drawable/ic_more_vert_white"
				android:scaleType="centerInside" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/midlin"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical"
			android:layout_weight="1">
			<ImageView
				android:id="@+id/imageview10"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:src="@drawable/default_image"
				android:scaleType="centerInside"
				android:layout_weight="1" />
			<LinearLayout
				android:id="@+id/linear18"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="#757575"
				android:gravity="center_vertical"
				android:orientation="horizontal"
				android:alpha="0.6"
				android:translationY="-180.0dp">
				<org.mark.axemojiview.view.AXEmojiTextView
					android:id="@+id/textview12"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="32dp"
					android:paddingTop="8dp"
					android:paddingRight="32dp"
					android:paddingBottom="8dp"
					android:gravity="center_horizontal"
					android:text="Caption"
					android:textSize="16sp"
					android:textColor="#FFFFFF"
					app:emojiSize="23sp" />
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/bottomlin"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="2dp"
			android:background="#FFFFFF"
			android:orientation="horizontal"
			android:fitsSystemWindows="true">
			<ImageView
				android:id="@+id/imageview5"
				android:layout_width="50dp"
				android:layout_height="50dp"
				android:padding="14dp"
				android:src="@drawable/ic_arrow_back_white"
				android:scaleType="centerInside"
				android:alpha="0.8" />
			<LinearLayout
				android:id="@+id/linear13"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical"
				android:layout_weight="1">
				<TextView
					android:id="@+id/textview10"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="0/0"
					android:textSize="14sp"
					android:textColor="#000000"
					android:alpha="0.6" />
			</LinearLayout>
			<ImageView
				android:id="@+id/imageview6"
				android:layout_width="50dp"
				android:layout_height="50dp"
				android:padding="14dp"
				android:src="@drawable/ic_arrow_back_white"
				android:scaleType="centerInside"
				android:rotation="180"
				android:alpha="0.8" />
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
