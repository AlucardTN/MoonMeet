<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/holder_fpc"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:padding="8dp"
		android:gravity="center_horizontal|center_vertical"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/main_dialog_holder"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="20dp"
			android:background="#FFFFFF"
			android:orientation="vertical">
			<TextView
				android:id="@+id/fingername"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:paddingLeft="8dp"
				android:paddingTop="8dp"
				android:paddingRight="8dp"
				android:paddingBottom="5dp"
				android:gravity="center_horizontal|center_vertical"
				android:text="Moon Meet"
				android:textSize="18sp"
				android:textColor="#000000"
				android:alpha="0.8" />
			<TextView
				android:id="@+id/hint"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:paddingLeft="8dp"
				android:paddingTop="7dp"
				android:paddingRight="8dp"
				android:paddingBottom="8dp"
				android:gravity="center_horizontal|center_vertical"
				android:text="Confirm fingerprint to continue"
				android:textSize="14sp"
				android:textColor="#000000"
				android:alpha="0.6" />
			<LinearLayout
				android:id="@+id/image_sesnor_holder"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="4dp"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/touch_sensor_image"
					android:focusable="false"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="1dp"
					android:src="@drawable/ic_fp"
					android:scaleType="center" />
				<TextView
					android:id="@+id/touch_sensor_text"
					android:focusable="false"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="15dp"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:text="Touch Sensor"
					android:textSize="14sp"
					android:textColor="#212121"
					android:alpha="0.6" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/cancel_holder"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="4dp"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/space"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="horizontal"
					android:layout_weight="10" />
				<TextView
					android:id="@+id/cancel_sensor"
					android:focusable="false"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="Cancel"
					android:textSize="16sp"
					android:textColor="#193566" />
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
