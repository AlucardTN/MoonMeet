<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/topbar"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="#ECF0F3"
		android:gravity="center_vertical"
		android:orientation="horizontal">
		<ImageView
			android:id="@+id/back"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="14dp"
			android:src="@drawable/ic_arrow_back_white"
			android:scaleType="center" />
		<LinearLayout
			android:id="@+id/space_top"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:padding="8dp"
			android:orientation="horizontal" />
	</LinearLayout>
	<LinearLayout
		android:id="@+id/divider_top"
		android:layout_width="match_parent"
		android:layout_height="1dp"
		android:padding="8dp"
		android:background="#DADADA"
		android:orientation="horizontal" />
	<LinearLayout
		android:id="@+id/background"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="#F5F5F5"
		android:orientation="horizontal"
		android:layout_weight="1">
		<androidx.core.widget.NestedScrollView
			android:id="@+id/NestedScroller"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:background="#E0E0E0"
			android:fillViewport="true">
			<LinearLayout
				android:id="@+id/SemiHolder"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/tobuyimage_holder"
					android:layout_width="match_parent"
					android:layout_height="300dp"
					android:background="#ECF0F3"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/tobuyimage"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:src="@drawable/background_cargo"
						android:scaleType="centerInside" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/Base_Purshase_Linear"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="1dp"
					android:padding="8dp"
					android:background="#ECF0F3"
					android:orientation="vertical">
					<TextView
						android:id="@+id/tobuymoney"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="8dp"
						android:paddingRight="8dp"
						android:paddingBottom="5dp"
						android:text="000,000 TND"
						android:textSize="18sp"
						android:textStyle="bold"
						android:textColor="#193566" />
					<TextView
						android:id="@+id/textview_cargo"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="5dp"
						android:paddingRight="8dp"
						android:paddingBottom="5dp"
						android:text="Name Here"
						android:textSize="16sp"
						android:textColor="#212121"
						android:alpha="0.89" />
					<LinearLayout
						android:id="@+id/star_holder"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:gravity="center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/cargo_star"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingTop="5dp"
							android:paddingRight="8dp"
							android:paddingBottom="8dp"
							android:text="⭐⭐⭐⭐⭐"
							android:textSize="12sp"
							android:textColor="#000000"
							android:layout_weight="1" />
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/specific_holder"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="5dp"
					android:padding="8dp"
					android:background="#ECF0F3"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/specific_base_text"
						android:layout_width="100dp"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Specifics"
						android:textSize="14sp"
						android:textColor="#757575" />
					<TextView
						android:id="@+id/specific_description"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Brand, your brand here"
						android:textSize="12sp"
						android:textColor="#757575"
						android:layout_weight="1" />
					<ImageView
						android:id="@+id/specific_more_info_bsh"
						android:layout_width="30dp"
						android:layout_height="30dp"
						android:src="@drawable/chevron_right"
						android:scaleType="centerInside"
						android:visibility="gone" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/delivery_holder"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="5dp"
					android:padding="8dp"
					android:background="#ECF0F3"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/delivery_dummy_txt"
						android:layout_width="100dp"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Delivery"
						android:textSize="14sp"
						android:textColor="#757575" />
					<LinearLayout
						android:id="@+id/delivery_semi_holder"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:orientation="vertical"
						android:layout_weight="1">
						<LinearLayout
							android:id="@+id/delivery_first_text_holder"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/delivery_txt_base_desceiption"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Tunsia, and we accept the following payments method :"
								android:textSize="12sp"
								android:textColor="#757575" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/delivery_second_text_holder"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/delivery_under_txt"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="E-Dinar / D17"
								android:textSize="12sp"
								android:textColor="#616161"
								android:layout_weight="1" />
							<TextView
								android:id="@+id/tax_delivery_txt"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="+2,00 TND"
								android:textSize="12sp"
								android:textStyle="bold"
								android:textColor="#424242" />
						</LinearLayout>
					</LinearLayout>
					<ImageView
						android:id="@+id/open_delivery_bsh"
						android:layout_width="30dp"
						android:layout_height="30dp"
						android:src="@drawable/chevron_right"
						android:scaleType="centerInside"
						android:visibility="gone" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/semi_divider_holder"
					android:layout_width="match_parent"
					android:layout_height="1dp"
					android:paddingLeft="8dp"
					android:paddingRight="8dp"
					android:background="#FFFFFF"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/divider"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="8dp"
						android:layout_marginRight="8dp"
						android:padding="8dp"
						android:background="#F5F5F5"
						android:orientation="horizontal" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/condition_holder"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="5dp"
					android:padding="8dp"
					android:background="#ECF0F3"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/condition_dummy_text"
						android:layout_width="100dp"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Condition"
						android:textSize="14sp"
						android:textColor="#757575" />
					<LinearLayout
						android:id="@+id/condition_all_things_holder"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:orientation="vertical"
						android:layout_weight="1">
						<LinearLayout
							android:id="@+id/condition_1_checked_holder"
							android:layout_width="match_parent"
							android:layout_height="25dp"
							android:paddingLeft="8dp"
							android:paddingRight="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/checkbox_image_1"
								android:layout_width="10dp"
								android:layout_height="10dp"
								android:src="@drawable/strings"
								android:scaleType="centerInside" />
							<TextView
								android:id="@+id/right_the_checkbox_image_1"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:text="100% genuine product"
								android:textSize="12sp"
								android:textColor="#616161" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/condition_2_checked_holder"
							android:layout_width="match_parent"
							android:layout_height="25dp"
							android:paddingLeft="8dp"
							android:paddingRight="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal"
							android:visibility="gone">
							<ImageView
								android:id="@+id/checkbox_image_2"
								android:layout_width="10dp"
								android:layout_height="10dp"
								android:src="@drawable/strings"
								android:scaleType="centerInside" />
							<TextView
								android:id="@+id/right_the_checkbox_image_2"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:text="Easy returns within 15 days"
								android:textSize="12sp"
								android:textColor="#616161" />
						</LinearLayout>
						<TextView
							android:id="@+id/condition_before_last_condition"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="26dp"
							android:paddingRight="8dp"
							android:paddingBottom="8dp"
							android:text="The reason for changing hearts cannot be used.  Only products that have not been opened and in perfect condition."
							android:textSize="10sp"
							android:textColor="#757575"
							android:visibility="gone" />
						<TextView
							android:id="@+id/condition_last_description"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="19dp"
							android:paddingRight="8dp"
							android:paddingBottom="8dp"
							android:text="You can only pay with E-Dinar Credit Card or using D17 application."
							android:textSize="12sp"
							android:textColor="#616161" />
					</LinearLayout>
					<ImageView
						android:id="@+id/open_condition_bsh"
						android:layout_width="30dp"
						android:layout_height="30dp"
						android:src="@drawable/chevron_right"
						android:scaleType="centerInside"
						android:visibility="gone" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/shop_infos_holder"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="5dp"
					android:background="#ECF0F3"
					android:gravity="center_horizontal"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/shop_helper"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:gravity="center_vertical"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/shop_image_holder"
							android:layout_width="40dp"
							android:layout_height="40dp"
							android:layout_margin="8dp"
							android:padding="5dp"
							android:background="#ECF0F3"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/shop_image"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:src="@drawable/k7shop"
								android:scaleType="centerInside" />
						</LinearLayout>
						<TextView
							android:id="@+id/shop_helper_name"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Ka7la Shop"
							android:textSize="16sp"
							android:textColor="#424242"
							android:layout_weight="1" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/rating_holder"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/shop_rating_holder"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:gravity="center_horizontal"
							android:orientation="vertical">
							<TextView
								android:id="@+id/shop_rating_percentage"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="8dp"
								android:paddingRight="8dp"
								android:text="95%"
								android:textSize="20sp"
								android:textColor="#424242"
								android:alpha="0.95" />
							<TextView
								android:id="@+id/shop_rating_txt"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingRight="8dp"
								android:paddingBottom="8dp"
								android:text="Shop rating"
								android:textSize="10sp"
								android:textColor="#757575" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/space_between_rating1"
							android:layout_width="1dp"
							android:layout_height="match_parent"
							android:layout_marginTop="10dp"
							android:layout_marginBottom="10dp"
							android:padding="8dp"
							android:background="#F5F5F5"
							android:orientation="vertical" />
						<LinearLayout
							android:id="@+id/delivery_on_time_holder"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:gravity="center_horizontal"
							android:orientation="vertical">
							<TextView
								android:id="@+id/delivery_on_time_percentage"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="8dp"
								android:paddingRight="8dp"
								android:text="97%"
								android:textSize="20sp"
								android:textColor="#424242"
								android:alpha="0.95" />
							<TextView
								android:id="@+id/delivery_on_time_txt"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingRight="8dp"
								android:paddingBottom="8dp"
								android:text="Delivery on time"
								android:textSize="10sp"
								android:textColor="#757575" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/space_beetwen_rating_2"
							android:layout_width="1dp"
							android:layout_height="match_parent"
							android:layout_marginTop="10dp"
							android:layout_marginBottom="10dp"
							android:padding="8dp"
							android:background="#F5F5F5"
							android:orientation="vertical" />
						<LinearLayout
							android:id="@+id/chat_response_holder"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:gravity="center_horizontal"
							android:orientation="vertical">
							<TextView
								android:id="@+id/chat_response_rate_percentage"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="8dp"
								android:paddingRight="8dp"
								android:text="93%"
								android:textSize="20sp"
								android:textColor="#424242"
								android:alpha="0.95" />
							<TextView
								android:id="@+id/chat_response_rate_txt"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingRight="8dp"
								android:paddingBottom="8dp"
								android:text="Chat response rate"
								android:textSize="10sp"
								android:textColor="#757575" />
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/ka7la_divider"
						android:layout_width="match_parent"
						android:layout_height="1dp"
						android:padding="8dp"
						android:background="#DADADA"
						android:orientation="horizontal" />
					<TextView
						android:id="@+id/gotoshop_textview"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Go to Shop"
						android:textSize="12sp"
						android:textColor="#193566" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/the_all_recommendation_holder"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="5dp"
					android:background="#FFFFFF"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/base_recommendation_linear_text_holder"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:background="#ECF0F3"
						android:orientation="horizontal"
						android:visibility="gone">
						<LinearLayout
							android:id="@+id/recommendation_text_holder"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical"
							android:layout_weight="1">
							<TextView
								android:id="@+id/textview_recommend"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Recommended for you"
								android:textSize="14sp"
								android:textColor="#193566" />
							<LinearLayout
								android:id="@+id/recommend"
								android:layout_width="wrap_content"
								android:layout_height="2dp"
								android:padding="8dp"
								android:background="#193566"
								android:orientation="horizontal" />
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/recommendation_linear"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:background="#ECF0F3"
						android:orientation="horizontal"
						android:visibility="gone">
						<LinearLayout
							android:id="@+id/recommend_page"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:orientation="vertical">
							<LinearLayout
								android:id="@+id/recommendation_1_2_holder"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="10dp"
								android:gravity="center_horizontal"
								android:orientation="horizontal">
								<LinearLayout
									android:id="@+id/recommendation_1_holder_cd"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_marginRight="2dp"
									android:gravity="center_horizontal"
									android:orientation="vertical">
									<LinearLayout
										android:id="@+id/recommendation_1_image_holder"
										android:layout_width="180dp"
										android:layout_height="wrap_content"
										android:layout_margin="8dp"
										android:padding="5dp"
										android:orientation="horizontal">
										<ImageView
											android:id="@+id/recommendation_1_image"
											android:layout_width="match_parent"
											android:layout_height="130dp"
											android:src="@drawable/background_cargo"
											android:scaleType="centerInside" />
									</LinearLayout>
									<TextView
										android:id="@+id/recommendation_1_money"
										android:layout_width="wrap_content"
										android:layout_height="wrap_content"
										android:paddingLeft="8dp"
										android:paddingRight="8dp"
										android:paddingBottom="8dp"
										android:text="27,790 TND"
										android:textSize="14sp"
										android:textColor="#193566" />
								</LinearLayout>
								<LinearLayout
									android:id="@+id/recommendation_2_holder_cd"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_marginLeft="2dp"
									android:gravity="center_horizontal"
									android:orientation="vertical">
									<LinearLayout
										android:id="@+id/recommendation_2_image_holder"
										android:layout_width="180dp"
										android:layout_height="wrap_content"
										android:layout_margin="8dp"
										android:padding="5dp"
										android:orientation="horizontal">
										<ImageView
											android:id="@+id/recommendation_2_image"
											android:layout_width="match_parent"
											android:layout_height="130dp"
											android:src="@drawable/background_cargo"
											android:scaleType="centerInside" />
									</LinearLayout>
									<TextView
										android:id="@+id/recommendation_2_money"
										android:layout_width="wrap_content"
										android:layout_height="wrap_content"
										android:paddingLeft="8dp"
										android:paddingRight="8dp"
										android:paddingBottom="8dp"
										android:text="53,290 TND"
										android:textSize="14sp"
										android:textColor="#193566" />
								</LinearLayout>
							</LinearLayout>
							<LinearLayout
								android:id="@+id/recommendation_3_4_holder"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="10dp"
								android:gravity="center_horizontal"
								android:orientation="horizontal">
								<LinearLayout
									android:id="@+id/recommendation_3_holder_cd"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_marginRight="2dp"
									android:gravity="center_horizontal"
									android:orientation="vertical">
									<LinearLayout
										android:id="@+id/recommendation_3_image_holder"
										android:layout_width="180dp"
										android:layout_height="wrap_content"
										android:layout_margin="8dp"
										android:padding="5dp"
										android:orientation="horizontal">
										<ImageView
											android:id="@+id/recommendation_3_image"
											android:layout_width="match_parent"
											android:layout_height="130dp"
