<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/main_holder"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="@android:color/transparent"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/second_holder"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:background="@android:color/transparent"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/top_lin"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:background="#ECF0F3"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/ttl1_txt"
					android:focusable="false"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="8dp"
					android:paddingRight="6dp"
					android:paddingBottom="8dp"
					android:text="By"
					android:textSize="20sp"
					android:textColor="#000000"
					android:singleLine="true"
					android:alpha="0.8" />
				<TextView
					android:id="@+id/ttl2_txt"
					android:focusable="false"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:paddingTop="8dp"
					android:paddingRight="8dp"
					android:paddingBottom="8dp"
					android:text="MoonMeetUser"
					android:textSize="20sp"
					android:textColor="#193566"
					android:singleLine="true" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/message_lin"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="#ECF0F3"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/itemmessage"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="4dp"
					android:paddingRight="8dp"
					android:paddingBottom="4dp"
					android:background="#ECF0F3"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<org.mark.axemojiview.view.AXEmojiTextView
						android:id="@+id/message_text"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="Message"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#000000"
						android:lines="2"
						android:alpha="0.8"
						android:layout_weight="1"
						android:maxLength="30"
						app:emojiSize="20sp" />
					<LinearLayout
						android:id="@+id/message_lin_time"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:gravity="center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/time"
							android:focusable="false"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:text="Time"
							android:textSize="12sp"
							android:textStyle="bold"
							android:textColor="#000000"
							android:alpha="0.8" />
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/item1_lin"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="#ECF0F3"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/item1"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="4dp"
					android:paddingRight="8dp"
					android:paddingBottom="4dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/item1_txt"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Item1"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#000000"
						android:alpha="0.8"
						android:layout_weight="1" />
					<ImageView
						android:id="@+id/item1_img"
						android:focusable="false"
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:padding="14dp"
						android:src="@drawable/default_image"
						android:scaleType="centerInside"
						android:alpha="0.8" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/item2_lin"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="#ECF0F3"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/item2"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="4dp"
					android:paddingRight="8dp"
					android:paddingBottom="4dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/item2_txt"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Item2"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#000000"
						android:alpha="0.8"
						android:layout_weight="1" />
					<ImageView
						android:id="@+id/item2_img"
						android:focusable="false"
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:padding="14dp"
						android:src="@drawable/default_image"
						android:scaleType="centerInside"
						android:alpha="0.8" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/item3_lin"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="#ECF0F3"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/item3"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="4dp"
					android:paddingRight="8dp"
					android:paddingBottom="4dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/item3_txt"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Item3"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#000000"
						android:alpha="0.8"
						android:layout_weight="1" />
					<ImageView
						android:id="@+id/item3_img"
						android:focusable="false"
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:padding="14dp"
						android:src="@drawable/default_image"
						android:scaleType="centerInside"
						android:alpha="0.8" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/item4_lin"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="#ECF0F3"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/item4"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="4dp"
					android:paddingRight="8dp"
					android:paddingBottom="4dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/item4_txt"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Item4"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#000000"
						android:alpha="0.8"
						android:layout_weight="1" />
					<ImageView
						android:id="@+id/item4_img"
						android:focusable="false"
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:padding="14dp"
						android:src="@drawable/default_image"
						android:scaleType="centerInside"
						android:alpha="0.8" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/item5_lin"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/item5"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="4dp"
					android:paddingRight="8dp"
					android:paddingBottom="4dp"
					android:background="#ECF0F3"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/item5_txt"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Item5"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#000000"
						android:alpha="0.8"
						android:layout_weight="1" />
					<ImageView
						android:id="@+id/item5_img"
						android:focusable="false"
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:padding="14dp"
						android:src="@drawable/default_image"
						android:scaleType="centerInside"
						android:alpha="0.8" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear1"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:paddingLeft="8dp"
				android:paddingTop="8dp"
				android:paddingRight="8dp"
				android:paddingBottom="16dp"
				android:orientation="horizontal" />
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
