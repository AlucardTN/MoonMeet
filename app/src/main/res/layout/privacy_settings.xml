<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/bar"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="#ECF0F3"
		android:gravity="center_vertical"
		android:orientation="horizontal"
		android:layout_gravity="center_vertical">
		<com.google.android.material.imageview.ShapeableImageView
			android:id="@+id/back"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:padding="14dp"
			android:src="@drawable/ic_arrow_back_white" />
		<com.google.android.material.textview.MaterialTextView
			android:id="@+id/privacy_topbar"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:padding="14dp"
			android:gravity="center_vertical"
			android:text="Privacy and Security"
			android:textSize="18sp"
			android:textColor="#193566" />
	</LinearLayout>
	<RelativeLayout
		android:id="@+id/relative_layout_holder"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="#ECF0F3"
		android:orientation="vertical"
		android:layout_weight="1">
		<androidx.appcompat.widget.FitWindowsLinearLayout
			android:id="@+id/fitslinearlayout"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="#ECF0F3"
			android:orientation="vertical"
			android:layout_weight="1">
			<androidx.appcompat.widget.LinearLayoutCompat
				android:id="@+id/linearcompat"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="#ECF0F3"
				android:orientation="vertical"
				android:layout_weight="1">
				<LinearLayout
					android:id="@+id/divider_topbar"
					android:layout_width="match_parent"
					android:layout_height="1dp"
					android:padding="8dp"
					android:background="#DADADA"
					android:orientation="horizontal" />
				<LinearLayout
					android:id="@+id/part1_holder"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="vertical">
					<TextView
						android:id="@+id/privacy_text"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Privacy"
						android:textSize="16sp"
						android:textColor="#193566" />
					<LinearLayout
						android:id="@+id/phonenumber_holder"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="4dp"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:clickable="true"
						android:focusable="true"
						android:foreground="?android:attr/selectableItemBackground">
						<TextView
							android:id="@+id/phonenumber"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingTop="1dp"
							android:paddingRight="5dp"
							android:paddingBottom="1dp"
							android:text="Phone Number"
							android:textSize="16sp"
							android:textColor="#193566" />
						<LinearLayout
							android:id="@+id/space_between"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:orientation="horizontal"
							android:layout_weight="1" />
						<TextView
							android:id="@+id/status_phone"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingTop="1dp"
							android:paddingBottom="1dp"
							android:text="Status"
							android:textSize="14sp"
							android:textColor="#000000"
							android:alpha="0.6" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/phonenumber_divider"
						android:layout_width="match_parent"
						android:layout_height="1dp"
						android:padding="8dp"
						android:background="#DADADA"
						android:orientation="horizontal"
						android:alpha="0.6" />
					<LinearLayout
						android:id="@+id/name_holder"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="4dp"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:clickable="true"
						android:focusable="true"
						android:foreground="?android:attr/selectableItemBackground">
						<TextView
							android:id="@+id/lastseen"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingTop="1dp"
							android:paddingRight="5dp"
							android:paddingBottom="1dp"
							android:text="Last Seen &amp; Online"
							android:textSize="16sp"
							android:textColor="#193566" />
						<LinearLayout
							android:id="@+id/space_between2"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:orientation="horizontal"
							android:layout_weight="1" />
						<TextView
							android:id="@+id/status_seen"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingTop="1dp"
							android:paddingBottom="1dp"
							android:text="Status"
							android:textSize="14sp"
							android:textColor="#000000"
							android:alpha="0.6" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/last_seen_dividee"
						android:layout_width="match_parent"
						android:layout_height="1dp"
						android:padding="8dp"
						android:background="#DADADA"
						android:orientation="horizontal"
						android:alpha="0.6" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/tip_linear"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:background="#F0F0F0"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/tip_text"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingRight="8dp"
						android:paddingBottom="8dp"
						android:text="Change who can view your personal information."
						android:textSize="14sp"
						android:textColor="#828282" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/part2_holder"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="vertical">
					<TextView
						android:id="@+id/textview5"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Security"
						android:textSize="16sp"
						android:textColor="#193566" />
					<LinearLayout
						android:id="@+id/passcode_linear"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="4dp"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:clickable="true"
						android:focusable="true"
						android:foreground="?android:attr/selectableItemBackground">
						<TextView
							android:id="@+id/passcode_text"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingTop="1dp"
							android:paddingRight="5dp"
							android:paddingBottom="1dp"
							android:text="Passcode Lock"
							android:textSize="16sp"
							android:textColor="#193566" />
						<LinearLayout
							android:id="@+id/space_between3"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:orientation="horizontal"
							android:layout_weight="1" />
						<TextView
							android:id="@+id/nothing"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingTop="1dp"
							android:paddingBottom="1dp"
							android:text="Clear Passcode"
							android:textSize="14sp"
							android:textColor="#000000"
							android:alpha="0.6" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/passcode_divider"
						android:layout_width="match_parent"
						android:layout_height="1dp"
						android:padding="8dp"
						android:background="#DADADA"
						android:orientation="horizontal"
						android:alpha="0.6" />
					<LinearLayout
						android:id="@+id/activesession_holder"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="4dp"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:clickable="true"
						android:focusable="true"
						android:foreground="?android:attr/selectableItemBackground">
						<TextView
							android:id="@+id/active_session"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingTop="1dp"
							android:paddingRight="5dp"
							android:paddingBottom="1dp"
							android:text="Active Session"
							android:textSize="16sp"
							android:textColor="#193566" />
						<LinearLayout
							android:id="@+id/space_between4"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:orientation="horizontal"
							android:layout_weight="1" />
						<TextView
							android:id="@+id/nothing2"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingTop="1dp"
							android:paddingBottom="1dp"
							android:text="Status"
							android:textSize="14sp"
							android:textColor="#000000"
							android:alpha="0.6"
							android:visibility="invisible" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/activesession_divider"
						android:layout_width="match_parent"
						android:layout_height="1dp"
						android:padding="8dp"
						android:background="#DADADA"
						android:orientation="horizontal"
						android:alpha="0.6" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/tip_linear2"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:background="#F0F0F0"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/tip_text2"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingRight="8dp"
						android:paddingBottom="8dp"
						android:text="Control your session on other devices."
						android:textSize="14sp"
						android:textColor="#828282" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/space"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:orientation="vertical"
					android:layout_weight="1" />
			</androidx.appcompat.widget.LinearLayoutCompat>
		</androidx.appcompat.widget.FitWindowsLinearLayout>
	</RelativeLayout>
</LinearLayout>
